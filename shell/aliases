# aliases
alias dc='docker-compose'
alias dfiles-sync='~/dotfiles/setup.sh'
alias dfiles='nvim ~/dotfiles'
alias git-blast='python ~/.git-blast'
alias god='git'
alias gut='git'
alias http='python -m SimpleHTTPServer 3000'
alias pgstart='postgres -D /usr/local/var/postgres'
alias pre-commit='sh ~/.pre-commit'
alias pull-request='export PR_LINK=$(hub pull-request) && open $PR_LINK'
alias reload='exec $SHELL -l'
alias rm-dstore='find . -name ".DS_Store" -type f -delete'
alias v='nvim'
alias vim='nvim'
alias hf='sudo nvim /etc/hosts'
