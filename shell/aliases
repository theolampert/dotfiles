# aliases
alias subl='open -a /Applications/Sublime\ Text.app'
alias atom='open -a /Applications/Atom.app'
alias vs='open -a /Applications/Visual\ Studio\ Code.app'
alias ls='ls -l -G'
alias reload='exec $SHELL -l'
alias clearatom='rm -rf ~/.atom/storage'
alias slack='open -a /Applications/Slack.app'
alias activate='source venv/bin/activate'
alias chrome='open -a /Applications/Google\ Chrome.app'
alias pgstart='postgres -D /usr/local/var/postgres'
alias git-blast='python ~/.git-blast'
alias http='python -m SimpleHTTPServer 8000'
alias god='git'
alias gut='git'
alias rm-dstore='find . -name ".DS_Store" -type f -delete'
alias v='vim'
alias dfiles='vim ~/dotfiles'
alias dfiles-sync='~/dotfiles/setup.sh && reload'
alias python2='python'
